{{#if parameters}}
{{#if @root.useOptions~}}
{{#each parameters}}
    {{#equals name 'namespace'}}
          {{convertGenericType type}} _nameSpace {{#if default}} = {{{default}}}{{/if}},
            {{else}}
             {{convertGenericType type}} {{{name}}} {{#if default}} = {{{default}}}{{/if}},
    {{/equals}}
{{/each}}
{{~else}}
{{#each parameters}}
{{{name}}}{{>isRequired}}: {{>type}}{{#if default}} = {{{default}}}{{/if}},
{{/each}}
{{/if}}
{{/if}}
